  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/app.ts",
    "dev2": "tsx -r tsconfig-paths/register src/app2_test.ts",
    "build": "rm -rf build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && ts-node ./build/src/app.js",
    "start": "pm2 start ./build/src/app.js -i 5 --attach --watch",
    "stop": "pm2 stop all",
    "delete": "pm2 delete all",
    "build2": "rm -rf build && tsc --project tsconfig.json && tsc-alias -p tsconfig.json && tsx src/app.ts",
    "dev3": "nodemon -r tsconfig-paths/register src/app.ts | pino-pretty -c",
    "test": "jest --watchAll=false",
    "test2": "jest --coverage=true -w=1 --forceExit --detectOpenHandles --watchAll=false",
    "lint:check": "eslint src --ext ts,tsx,js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext ts,tsx,js,jsx --fix",
    "prettier:check": "prettier --check 'src/**/*.{ts,tsx,js,jsx,json}'",
    "prettier:fix": "prettier --write 'src/**/*.{ts,tsx,js,jsx,json}'"
